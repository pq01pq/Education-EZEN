<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet>
	<servlet-name>hello</servlet-name>
	<servlet-class>HelloServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>hello</servlet-name>
	<url-pattern>/hello.do</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>contact</servlet-name>
	<servlet-class>servlet.ContactServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>contact</servlet-name>
	<url-pattern>/contact.do</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>memo</servlet-name>
	<servlet-class>servlet.MemoServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>memo</servlet-name>
	<url-pattern>/memo.do</url-pattern>
</servlet-mapping>
</web-app>